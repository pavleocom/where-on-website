{
    "name": "pavleocom/where-on-website",
    "description": "Scans internal web pages of a website and returns a list of urls of web pages containing the searched text.",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel"
        }
    ],
    "require": {
        "psr/http-message": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "nyholm/psr7": "^1.3",
        "paquettg/php-html-parser": "^3.1"
    },
	"autoload": {
	"psr-4": {
		"WhereOnWebsite\\": "src/"
		}	
    },
    "autoload-dev": {
        "psr-4": {
            "WhereOnWebsite\\Tests\\": "tests/"
        }
    }
}
